<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <LocalDebuggerWorkingDirectory>$(ProjectDir)..\..\Debug</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>$(ProjectDir)..\..\Debug\ppp.exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>--mode=client --lwip=yes --tun-ip=10.0.0.2 --tun-gw=10.0.0.0 --tun-mask=30 --tun-host=yes --tun-vnet=yes --tun-static=yes --block-quic=yes --bypass-iplist=ip.txt</LocalDebuggerCommandArguments>
